{
  "name": "mike-farrow-site",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently --prefix ' ' -c 'bgGreen,bgRed' \"next dev --webpack\" \"npm run watch\"",
    "prebuild": "npm run generate:types && npm run lint && npm run check",
    "build": "next build --webpack",
    "start": "next start",
    "watch": "chokidar ./node_modules/@mikefarrow/cms/schema.json ./src/lib/sanity/queries --initial -c 'npm run generate:types'",
    "lint": "eslint .",
    "check": "tsc --noEmit",
    "generate:types": "sanity typegen generate",
    "prepare": "husky"
  },
  "dependencies": {
    "@mikefarrow/cms": "github:michaelfarrow/mike-farrow-cms#66c2fdfb4aa3e425203da2d1dfe904cf04a61463",
    "@mikefarrow/prettier": "github:michaelfarrow/prettier#3.6.2-1",
    "@sanity/image-url": "^1.2.0",
    "@uidotdev/usehooks": "^2.4.1",
    "change-case": "^5.4.4",
    "clsx": "^2.1.1",
    "format-duration": "^3.0.2",
    "get-video-id": "^4.1.7",
    "lodash-es": "^4.17.21",
    "merge-refs": "^2.0.0",
    "next": "16.0.1",
    "next-sanity": "^11.5.1",
    "num2fraction": "^1.2.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-player": "^3.3.3",
    "react-refractor": "^4.0.0",
    "react-timing-hooks": "^5.1.1",
    "refractor": "^5.0.0",
    "styled-components": "^6.1.19",
    "title-case": "^4.3.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next/bundle-analyzer": "16.0.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.14",
    "@types/num2fraction": "^1.2.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^1.0.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "eslint-config-next": "16.0.1",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "sass": "^1.93.2",
    "tailwindcss": "^3.4.18",
    "type-fest": "^5.0.1",
    "typescript": "^5.9.3",
    "typescript-plugin-css-modules": "^5.2.0"
  }
}
